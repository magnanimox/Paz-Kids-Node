{
  "name": "paz-kids-node",
  "version": 2,
  "public": true,
  "functions": {
    "src/server.ts": {
      "memory": 100
    },
  },
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/server.ts"
    }
  ]
}